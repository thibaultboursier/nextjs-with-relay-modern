{
  "name": "nextjs-with-relay-modern",
  "version": "0.1.0",
  "description": "Handling SSR with Relay Modern inside NextJS project",
  "main": "index.js",
  "scripts": {
    "dev": "npm run relay && next",
    "build": "next build",
    "start": "next start",
    "relay": "npm run relay:schema && npm run relay:compile",
    "relay:compile": "relay-compiler",
    "relay:compile-watch": "relay-compiler --watch",
    "relay:schema": "graphql get-schema -e dev"
  },
  "keywords": [
    "react",
    "relay",
    "relay-modern",
    "ssr",
    "next",
    "graphql-client"
  ],
  "author": {
    "name": "Thibault Boursier"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thibaultboursier/nextjs-with-relay-modern.git"
  },
  "license": "ISC",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "bootstrap": "^4.4.1",
    "graphql-cli": "^3.0.14",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^11.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reactstrap": "^8.1.1"
  },
  "devDependencies": {
    "babel-plugin-macros": "^2.6.2",
    "babel-plugin-relay": "^7.1.0",
    "graphql": "^14.5.8",
    "react-relay": "^7.1.0",
    "relay-compiler": "^7.1.0",
    "relay-config": "^7.1.0"
  }
}
